#include <stdio.h>
#include <stdlib.h>
typedef struct Tree {
	char data;
	struct Tree* lchild;
	struct Tree* rchild;
}Tree;

void createTree(Tree** T, char* data, int* index) {
	char ch;
	ch = data[*index];
	*index +=1;
	if (ch == '#') {
		*T = NULL;
	}
	else {
		*T = (Tree*)malloc(sizeof(Tree));
		(*T)->data = ch;
		createTree(&((*T)->lchild), data, index);
		createTree(&((*T)->rchild), data, index);
	}


	
}

void Preorder(Tree* T) {
	if (T == NULL) {
		return;
	}
	else {
		printf("%c ", T->data);
		Preorder(T->lchild);
		Preorder(T->rchild);
	}
}

int main() {
	Tree* T;
	int index = 0;
	char c;
	char ch[20];
	while ((c = getchar()) && c != '\n')
		ch[index++] = c;
	index = 0;
	createTree(&T, ch, &index);
	Preorder(T);
}
