#include <stdio.h>
#include <stdlib.h>
struct Node
{
	int num;
	Node* next;

};
struct Node* head;
void Insert(int a, int b) {
	int i;
	Node* temp = (Node*)malloc(sizeof(struct Node*));
	temp->num = a;
	temp->next = NULL;
	if (b == 1) {
		temp->next = head;
		head = temp;
		return;
	}
	Node* temp2 = head;
	for (i = 0; i < b - 2; i++) {
		temp2 = temp2->next;
	}
	temp->next = temp2->next;
	temp2->next = temp;
}
void Print() {
	Node* p=head;
	while (p != NULL) {
		printf("%d ", p->num);
		p = (*p).next;
	}

}
int main() {
	head = NULL;
	Insert(2, 1);
	Insert(3, 2);
	Insert(4, 1);
	Insert(5, 2);
	Print();
	return 0;
}

