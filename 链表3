#include <stdio.h>
#include <stdlib.h>
struct Node {
	int data;
	Node* next;
};//生成一个结构体
struct Node* head;  //定义头结点
Node* t = head;  // 让 t = 头结点
void Insert(int n) {
	Node* p = (Node*)malloc(sizeof(struct Node)); // 开辟一个内存空间
	p->data = n;
	p->next = NULL;
	if (head == NULL) {  // 当head等于null时，让head指向p；
		head = p;
		t = p;  // 一样
		return;
	}
	t->next = p;  //要想在结尾连上，此时t指向的是p1，就需要让t->next指向p2;
	t = p;  // 让t指向p2；达到循环
}
void Print() {
	Node* p = head;
	while (p != NULL) {
		printf("%d ", p->data);
		p = (*p).next;
	}
	printf("\n");

}是
void Change() {
	Node* y1 = head; //负责保存要改变的结点
	Node* y2 = head; //负责遍历
	Node* y = head;  //负责保存旧的结点
	if (y1 = head) { //当在头结点时
		y2 = y1->next;  //先让y2走到下一个结点
		y1->next = NULL;
	}
	while (y2 != NULL) { 
		y1 = y2;  // 让y1到y2的位置
		y2 = y2->next;  //再让y2走到下一个结点
		y1->next = y;  //再让y1改变，指向旧的
		y = y1; //再让y指向y1;
	}
	head = y1;
}

int main() {
	int n;
	head = NULL;
	Insert(2);
	Insert(4);
	Insert(6);
	Insert(5);
	printf("\n");
	Print();
	printf("翻转\n");
	Change();
	Print();
	return 0;
}
