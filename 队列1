#include <stdio.h>
#include <stdlib.h>
typedef struct Queue {
	int data;
	struct Queue* next;
	struct Queue* pre;
}Queue;

struct Queue* front = NULL;
struct Queue* rear = NULL;
void Enqueue(int n){
	struct Queue* temp = (Queue*)malloc(sizeof(Queue));
	temp->data = n;
	temp->next = front;
	if (front != NULL) {
		front = temp;
		temp->next->pre = temp;
	}
	else {
		rear = temp;
		rear->next = NULL;
		temp->pre = NULL;
		front = temp;
		return;
	}
	
}
void Qequeue(){
	Queue* temp=rear;
	rear = rear->pre;
	rear->next = NULL;
	temp->pre = NULL;
	free(temp);
}
int Lookfront() {
	if (front == NULL)return 0;
	return front->data;
}
int Empty() {
	if (front != NULL)return 1;
	return 0;
}
void Print() {
	Queue* t = front;
	while (t != NULL) {
		printf("%d ", t->data);
		t = t->next;
	}
	printf("\n");
	
}

int main() {
	Enqueue(1);
	Enqueue(2);
	Enqueue(3);
	Enqueue(4);
	Enqueue(5);
	Enqueue(6);
	Print();
	Qequeue(); 
	Qequeue(); 
	Print();
}
